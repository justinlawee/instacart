# .env.example

# AWS credentials
AWS_KEY_ID=your-aws-access-key
AWS_SECRET_KEY=your-aws-secret-key

# Snowflake connection
SNOWFLAKE_ACCOUNT=your-snowflake-account
SNOWFLAKE_USER=your-snowflake-username
SNOWFLAKE_AUTHENTICATOR=externalbrowser
SNOWFLAKE_ROLE=ACCOUNTADMIN
SNOWFLAKE_WAREHOUSE=INSTACART
SNOWFLAKE_DATABASE=INSTACART_DB
SNOWFLAKE_SCHEMA=RAW

# AWS credentials for S3 access
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_REGION=us-west-2
AWS_BUCKET=your-bucket-name
AWS_PREFIX=your/path/prefix  # optional
AWS_SESSION_TOKEN=your-session-token-if-using-temporary-keys
# AWS_ENDPOINT_URL=https://s3.us-west-2.amazonaws.com  # optional   